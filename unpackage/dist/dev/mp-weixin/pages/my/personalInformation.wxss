page {
	height: 100% !important;
	background: #f4f4f4;
}
.list_content_div {
  width: 701rpx;
  margin: 18rpx auto;
  background: #FFFFFF;
  overflow: hidden;
  border-radius: 20rpx;
}
.list_content_div .list_content_divList {
  overflow: hidden;
  height: 97rpx;
  border-bottom: 2rpx solid rgba(207, 207, 207, 0.22);
}
.list_content_div .list_content_divList text {
  float: left;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #1B1B1B;
  margin: 28rpx auto auto 40rpx;
}
.list_content_div .list_content_divList .smilingOn {
  float: right;
  margin: 33rpx 62rpx auto auto;
  color: #9F9F9F;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 300;
}
.list_content_div .list_content_divList .submit-arrow {
  width: 17rpx;
  height: 30rpx;
  float: left;
  margin: 27rpx auto auto 36rpx;
}

