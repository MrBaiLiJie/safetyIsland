page {
	height: 100% !important;
	background: #f4f4f4;
}
.dataStatistics_content {
  width: 701rpx;
  height: 577rpx;
  background: #FFFFFF;
  border-radius: 20rpx;
  margin: 21rpx auto;
}
.dataStatistics_content .dataStatistics_contentOne text {
  width: 118rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 300;
  color: #0A0A0A;
}
.dataStatistics_content .team_score {
  overflow: hidden;
  height: 115rpx;
}
.dataStatistics_content .team_score view {
  width: 158rpx;
  float: left;
  margin: 31rpx auto auto 21rpx;
  text-align: center;
}
.dataStatistics_content .team_score image {
  width: 86rpx;
  height: 84rpx;
}
.dataStatistics_content .team_score text {
  float: left;
  margin: 60rpx auto auto 46rpx;
}
.dataStatistics_content .team_scoreTit {
  overflow: hidden;
}
.dataStatistics_content .team_scoreTit view {
  width: 162rpx;
  float: left;
  margin: 27rpx auto auto 21rpx;
  text-align: center;
}
.dataStatistics_content .team_scoreTit text {
  float: left;
  margin: 27rpx auto auto 46rpx;
}
.ec-canvas {
  width: 680rpx;
  height: 800rpx;
  background: #FFFFFF;
  -webkit-border-radius: 20px;
          border-radius: 20px;
  margin: 21rpx auto;
  overflow: hidden;
  position: relative;
}
.ec-canvas .uni-ec-canvas {
  width: 98%;
  height: 84%;
  display: block;
  position: absolute;
  top: 12%;
}
.ec-canvas .ec-canvasTit {
  font-size: 30rpx;
  font-family: Noto Sans S Chinese;
  font-weight: 400;
  color: #0F0F0F;
  margin: 32rpx auto auto 34rpx;
}
.ec-canvas .legend {
  overflow: hidden;
  position: absolute;
  top: 10%;
}
.ec-canvas .legend .ec-canvas_legend {
  overflow: hidden;
  float: left;
  margin-left: 40rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.ec-canvas .legend .ec-canvas_legend .legend_left {
  font-size: 23rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #101010;
  float: left;
}
.ec-canvas .legend .ec-canvas_legend .legend_img {
  width: 101rpx;
  height: 49rpx;
  float: left;
}
.ec-canvas .legend .ec-canvas_legend .legend_right {
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #101010;
  float: left;
}
.ec-canvasTwo {
  width: 680rpx;
  height: 640rpx;
  background: #FFFFFF;
  -webkit-border-radius: 20px;
          border-radius: 20px;
  margin: 65rpx auto;
}
.ec-canvasTwo .uni-ec-canvas {
  width: 98%;
  height: 98%;
  display: block;
}

