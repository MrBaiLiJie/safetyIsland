page {
  padding-top: 22rpx;
  height: 100%;
  background-color: #F4F4F4;
}
.content-wrap {
  margin: 14rpx 32rpx 20rpx 32rpx;
  background-color: #ffffff;
  border-radius: 24rpx;
}
.content-wrap .uni-list-cell {
  padding: 28rpx 50rpx 30rpx 36rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content-wrap .uni-list-cell .uni-list-cell-left {
  width: 160rpx;
  color: #000000;
  font-size: 30rpx;
}
.content-wrap .uni-list-cell .uni-list-cell-db {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 26rpx;
  color: #797979;
  font-size: 30rpx;
}
.content-wrap .line {
  width: 100%;
  height: 1px;
  background-color: #CFCFCF;
}
.content-wrap .feedback-wrap {
  padding: 0 40rpx 36rpx 38rpx;
}
.content-wrap .feedback-wrap .uni-title {
  padding: 26rpx 0 20rpx 0;
  color: #000000;
  font-size: 30rpx;
}
.content-wrap .feedback-wrap .uni-textarea {
  min-height: 190rpx;
  padding: 24rpx 32rpx;
  background: #F6F6F6;
  color: #868686;
  font-size: 26rpx;
}
.content-wrap .feedback-wrap .subBtn-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
}
.content-wrap .feedback-wrap .subBtn-wrap .subBtn {
  margin: 0 auto;
  width: 513rpx;
  height: 109rpx;
}
.content-wrap .feedback-wrap .picture-record-list {
  margin: 0 -8rpx;
  overflow: hidden;
}
.content-wrap .feedback-wrap .picture-record-list .list-item {
  padding: 0 8rpx;
  float: left;
  width: 33.333%;
}
.content-wrap .feedback-wrap .picture-record-list .list-item .list-item-wrap {
  position: relative;
  height: 0;
  padding-top: 81%;
}
.content-wrap .feedback-wrap .picture-record-list .list-item .list-item-wrap .list-item-img {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
}

