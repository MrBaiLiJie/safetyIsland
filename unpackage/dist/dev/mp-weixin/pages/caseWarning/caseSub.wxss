page {
  padding-top: 20rpx;
  height: 100%;
  background-color: #F4F4F4;
}
.submit-wrap {
  margin: 0 32rpx;
  padding: 0 24rpx 0 32rpx;
  height: 78rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  /* 垂直居中 */
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  /* 水平居中 */
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  background-color: #ffffff;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
}
.submit-wrap .title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  color: #151515;
  font-size: 30rpx;
}
.submit-wrap .title image {
  margin-right: 20rpx;
  width: 37rpx;
  height: 41rpx;
}
.submit-wrap .submit-arrow {
  float: right;
  width: 17rpx;
  height: 30rpx;
}
.content-wrap {
  margin: 14rpx 32rpx 20rpx 32rpx;
  padding-top: 28rpx;
  background-color: #ffffff;
  -webkit-border-radius: 24rpx;
          border-radius: 24rpx;
}
.content-wrap .uni-list-cell {
  padding: 0 50rpx 30rpx 36rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.content-wrap .uni-list-cell .uni-list-cell-left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  /* 垂直居中 */
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #000000;
  font-size: 30rpx;
}
.content-wrap .uni-list-cell .uni-list-cell-db {
  padding: 0 24rpx 0 34rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  /* 垂直居中 */
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-left: 26rpx;
  border: 1px solid #696969;
  height: 52rpx;
  line-height: 52rpx;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
}
.content-wrap .uni-list-cell .uni-list-cell-db picker {
  position: relative;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.content-wrap .uni-list-cell .uni-list-cell-db picker .uni-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #888888;
  font-size: 26rpx;
}
.content-wrap .uni-list-cell .uni-list-cell-db picker .down-arrow {
  position: absolute;
  top: -6rpx;
  right: 0;
  width: 24rpx;
  height: 15.5rpx;
}
.content-wrap .uni-list-cell .uni-list-cell-db picker .down-arrow image {
  width: 100%;
  height: 100%;
}
.content-wrap .line {
  width: 100%;
  height: 1px;
  background-color: #CFCFCF;
}
.content-wrap .feedback-wrap {
  padding: 0 40rpx 36rpx 38rpx;
}
.content-wrap .feedback-wrap .uni-title {
  padding: 26rpx 0 20rpx 0;
  color: #000000;
  font-size: 30rpx;
}
.content-wrap .feedback-wrap .uni-textarea {
  width: 100%;
  height: 172rpx;
  padding: 24rpx 32rpx;
  background: #F6F6F6;
}
.content-wrap .feedback-wrap .uni-textarea .textarea {
  width: 100%;
  height: 100%;
}
.content-wrap .feedback-wrap .subBtn-wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
}
.content-wrap .feedback-wrap .subBtn-wrap .subBtn {
  margin: 0 auto;
  width: 513rpx;
  height: 109rpx;
}
.content-wrap .feedback-wrap .describe {
  min-height: 190rpx;
  padding: 24rpx 32rpx;
  background: #F6F6F6;
  color: #868686;
  font-size: 26rpx;
}

